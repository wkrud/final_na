<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="comment">
	<select id="selectCultureCommentList" resultType="comment">
		select
			*
		from
			comments
		where
			api_code = #{api_code}
	</select>
</mapper>

